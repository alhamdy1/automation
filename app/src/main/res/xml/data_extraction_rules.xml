<?xml version="1.0" encoding="utf-8"?>
<!--
    Data Extraction Rules for Android 12+
    
    SECURITY:
    - Disable all data extraction to prevent backup of sensitive data
    - This ensures encryption keys and logs are not backed up
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Disable cloud backup entirely -->
        <exclude domain="root" />
        <exclude domain="file" />
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="external" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Disable device-to-device transfer -->
        <exclude domain="root" />
        <exclude domain="file" />
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="external" />
    </device-transfer>
</data-extraction-rules>
